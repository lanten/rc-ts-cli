    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "default-gateway": "^6.0.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
