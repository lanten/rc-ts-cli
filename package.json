    "@types/node": "^16.11.12",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.5.0",
    "@types/webpack-env": "^1.16.3",
    "@types/webpackbar": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "address": "^1.1.2",
    "cross-env": "^7.0.3",
